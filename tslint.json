{
  "rules": {
    "ban": [true, ["fdescribe"], ["xdescribe"], ["fit"], ["xit"]],
    "curly": true,
    "class-name": true,
    "import-spacing": true,
    "indent": [
      true,
      "spaces"
    ],
    "max-line-length": [true, 100],
    "member-ordering": [
      true,
      { "order": "fields-first" }
    ],
    "no-any": false,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-internal-module": true,
    "no-trailing-whitespace": false,
    "no-var-keyword": true,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-empty": false,
    "no-string-literal": false,
    "no-use-before-declare": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": true,
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-module"
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
